{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    }
  },
  "apphosting": {
    "backendId": "okazje-plus-app",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "functions": [
    {
      "source": ".firebase/okazje-plus/functions",
      "codebase": "firebase-frameworks-okazje-plus"
    }
  ]
}
